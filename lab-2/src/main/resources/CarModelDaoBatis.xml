<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="is.tech.mybatis.CarModelMapper">
    <select id="getById" parameterType="long" resultType="is.tech.models.CarModel">
        select * from car_model where id = #{id}
    </select>
    <insert id="save" parameterType="is.tech.models.CarModel">
        insert into car_model (id,name,foundation_date)
        values(#{id},#{name},#{date})
    </insert>
</mapper>